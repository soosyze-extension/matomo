{
    "name": "soosyze-extension/matomo",
    "type": "soosyze-module",
    "description": "Measure your audience by adding the Matomo (Piwik) javascript tracking code to your site.",
    "version": "1.0",
    "autoload": {
        "psr-4": {
            "SoosyzeExtension\\Matomo\\": "/"
        }
    },
    "extra": {
        "soosyze-module": {
            "title": "Matomo",
            "package": "Analytics",
            "controllers": {
                "Matomo": "SoosyzeExtension\\Matomo\\Controller\\Matomo"
            },
            "icon": {
                "name": "fas fa-chart-line",
                "background-color": "#3450a3",
                "color": "#fff"
            }
        }
    }
}